/* Scroll Bar */
#scrollbar1 { width: auto; height: auto; margin: 8px 16px; }
#scrollbar1 .viewport { width: auto; height: 400px; overflow: hidden; position: relative; margin: 8px;}
#scrollbar1 .overview { list-style: none; position: absolute; left: 0; top: 0; padding: 0; margin: 0; }
#scrollbar1 .overview  ul li {list-style-type: none; display: inline;}
#scrollbar1 .overview img {padding: 8px 17px 8px 17px; height: 177px; width: 137px;}
#scrollbar1 .scrollbar{ background: transparent url(../m/scrollbar/bg-scrollbar-track-y.png) no-repeat 0 0; position: relative; background-position: 0 0; float: right; width: 15px; }
#scrollbar1 .track { background: transparent url(../m/scrollbar/bg-scrollbar-trackend-y.png) no-repeat 0 100%; height: 100%; width:15px; position: relative; }
#scrollbar1 .thumb { background: transparent url(../m/scrollbar/bg-scrollbar-thumb-y.png) no-repeat 50% 100%; height: 20px; width: 25px; cursor: pointer; overflow: hidden; position: absolute; top: 0; left: -5px; }
#scrollbar1 .thumb .end { background: transparent url(../m/scrollbar/bg-scrollbar-thumb-y.png) no-repeat 50% 0; overflow: hidden; height: 5px; width: 25px; }
#scrollbar1 .disable { display: none; }

/* Sales Phase */
#salesphase p {font-family: 'Palatino Linotype', 'Book Antiqua', Palatino, serif; padding: 10px 0 10px 0;}
#salesphase table img {display: block; margin: 0 auto;}
#salesphase h3 {margin: 15px 0 10px 15px;}
.salesoptions {width: 85%; margin: 10px auto 10px auto; border-top: 3px solid #9dd66d; border-bottom: 3px solid #9dd66d; padding: 5px; background-color: #f3fbed; font-size: 14px;}
.salesoptions ul li {list-style-type: none; display: inline; padding: 0 5px 0 5px;}
.salesoptions a, .salesoptions a:visited {text-decoration: none; color: #9dd66d}
.salesoptions a:hover {text-decoration: none; color: #82b657;}
.progbar {margin: 5px 0 5px 65px;}
#advertiserlist {width: 85%; background-color: #92b277; color: white; margin: 0 auto;}
#advertiserlist h2 {color: white; padding: 15px;}
#salesphase table {width: 85%; margin: 0 auto 20px auto; font-size: 14px; border: 0px; background-color: #e9f7dd;border-collapse:collapse;}
#salesphase tbody td {padding: 15px; text-align: left; border-right: -5px; border-left: 0px; border-bottom: 2px solid #9dd66d; }
#salesphase tbody th {padding: 15px; text-align: left; background-color: #cde6b9; border-bottom: 4px solid #9dd66d; font-size: 16px; color: #8d599a;}
#salesphase tbody tr {border: 0px;}
#salesphase tr:nth-child(even) {background: #f6fcf1; color: #8d599a;}
#salesphase tr:nth-child(odd) {background: #e9f7dd; color: black;}
#salesphase select {width: 75px;}
#salesphase form #demo2 {margin: 20px 0 0 0;}
.blockbutton {margin: 10px 0 0 0; display: block;}
.swatches {cursor: pointer;}
ul.timeline li {font-size: 28px; font-weight: bold; padding: 15px 0 15px 0; margin: 0 0 0 200px; list-style-type: none}
#jOverlayContent {padding: 20px; background-color: white; }


/* CSS Button */
button.css3button {
    font-family: Arial, Helvetica, sans-serif;
    font-size: 10px;
    text-transform: uppercase;
    color: #864e95;
    cursor: pointer;
    padding: 4px 14px;
    background: -moz-linear-gradient(
        top,
        #f6f6f6 0%,
        #dedede);
    background: -webkit-gradient(
        linear, left top, left bottom, 
        from(#f6f6f6),
        to(#dedede));
    border-radius: 7px;
    -moz-border-radius: 7px;
    -webkit-border-radius: 7px;
    border: 2px solid #9f9f9f;
    -moz-box-shadow:
        0px 2px 5px rgba(000,000,000,0.2),
        inset 0px 0px 0px rgba(255,255,255,0);
    -webkit-box-shadow:
        0px 2px 5px rgba(000,000,000,0.2),
        inset 0px 0px 0px rgba(255,255,255,0);
    text-shadow:
        0px 0px 0px rgba(255,255,255,0),
        0px 0px 0px rgba(255,255,255,0);
}


button.css3button:hover {
    font-family: Arial, Helvetica, sans-serif;
    font-size: 10px;
