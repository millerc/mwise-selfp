.show_hide2 img {cursor: pointer;}
.show_hide3 img {cursor: pointer;}
.show_hide4 img {cursor: pointer;}
.show_hide5 img {cursor: pointer;}
.spreadselector img {padding: 5px 15px 5px 15px;}

/* Overlay Box */
.overlay {
  display: none;
  z-index: 10000;
  background-color: #ffffff;
  width: 675px;
  min-height: 200px;
  border: 1px solid #666;
  -moz-box-shadow: 0 0 90px 5px #000;
  -webkit-box-shadow: 0 0 90px #000;
  padding: 30px;
}
.overlay .close {
  background-image: url(../m/overlay/close.png);
  position: absolute;
  right: -15px;
  top: -15px;
  cursor: pointer;
  height: 35px;
  width: 35px;
}

/* Overlay Spread */
.overlay-spread {
  display: none;
  z-index: 10000;
  background-color: #ffffff;
  width: auto;
  min-height: auto;
  border: 1px solid #666;
  -moz-box-shadow: 0 0 90px 5px #000;
  -webkit-box-shadow: 0 0 90px #000;
  padding: 30px;
}
.overlay-spread .close {
  background-image: url(../m/overlay/close.png);
  position: absolute;
  right: -15px;
  top: -15px;
  cursor: pointer;
  height: 35px;
  width: 35px;
}
/* Overlay Gallery */
#gallery h1 {color: #864e95; text-align: center;}
#gallery ul li {list-style-type: none; display: inline;}
#gallery img {padding: 10px 25px 10px 30px;}
/* Header */
#header {
  width: 964px;
  min-height: 30px;
  background: #6a6a6a;
  /* Old browsers */

  background: -moz-linear-gradient(top, #6a6a6a 0%, #6a6a6a 6%, #3d3d3d 22%, #3d3d3d 71%, #585858 100%);
  /* FF3.6+ */

  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #6a6a6a), color-stop(6%, #6a6a6a), color-stop(22%, #3d3d3d), color-stop(71%, #3d3d3d), color-stop(100%, #585858));
  /* Chrome,Safari4+ */

  background: -webkit-linear-gradient(top, #6a6a6a 0%, #6a6a6a 6%, #3d3d3d 22%, #3d3d3d 71%, #585858 100%);
  /* Chrome10+,Safari5.1+ */

  background: -o-linear-gradient(top, #6a6a6a 0%, #6a6a6a 6%, #3d3d3d 22%, #3d3d3d 71%, #585858 100%);
  /* Opera11.10+ */

  background: -ms-linear-gradient(top, #6a6a6a 0%, #6a6a6a 6%, #3d3d3d 22%, #3d3d3d 71%, #585858 100%);
  /* IE10+ */

  background: linear-gradient(top, #6a6a6a 0%, #6a6a6a 6%, #3d3d3d 22%, #3d3d3d 71%, #585858 100%);
  /* W3C */
  border-bottom: 2px solid #000000;
  -webkit-box-shadow: rgba(0, 0, 0, 0.3) 0 4px 6px;
  -moz-box-shadow: rgba(0, 0, 0, 0.3) 0 4px 6px;
  position: relative;
}
